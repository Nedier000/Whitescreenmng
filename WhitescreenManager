local Button = sps:CreateButton({
	Name = "Run Script",
	Callback = function()
		getgenv().config = {
            autoFish = true,
            placetoFish = nil,
            autoPresents = true,
            invisWater = true,

            userToMail = _G.Accmail,
            autoMail = _G.Automail,
            sendHuges = _G.Guihuge,
            sendShards = _G.Autoshard,

            sendDiamonds = _G.Autogem,
            minShards = _G.Minshard,
            minDiamonds = _G.Mingem,
            keepDiamonds = _G.Keepgem,
       }
       loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/2c340ba7f63eb21c2e772c76d8d077be.lua"))()
	end,
})

