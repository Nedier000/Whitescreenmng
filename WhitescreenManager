local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

getgenv().SecureMode = true

local Window = Rayfield:CreateWindow({
	Name = "Whitescreen Manager",
	LoadingTitle = "WHITESCREEN",
	LoadingSubtitle = "by Minh Lts",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = "Whitescreen",
		FileName = "whitescreen"
    }
})

local Tab = Window:CreateTab("Main", 4483362458)

_G.Setfps = "15"

local Input = Tab:CreateInput({
	Name = "Set Fps Cap(default is 15)",
	PlaceholderText = "Input",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
	    _G.Setfps = Text
	end,
})

local Button = Tab:CreateButton({
	Name = "WhiteScreen ON",
	Callback = function()
		setfpscap(_G.Setfps)
        wait(1)
        local RunService = game:GetService("RunService")RunService:Set3dRenderingEnabled(false)
	end,
})

local Button = Tab:CreateButton({
	Name = "Whitecsreen OFF(not turn off fpscap)",
	Callback = function()
		setfpscap(_G.Setfps)
        wait(1)
        local RunService = game:GetService("RunService")RunService:Set3dRenderingEnabled(true)
	end,
})


local Button = Tab:CreateButton({
	Name = "Anti Afk(not work,đang fix)",
	Callback = function()
		local player = game.Players.LocalPlayer
		wait(30)
		player.Jump = true
	end,
})

local Button = Tab:CreateButton({
	Name = "Tắt menu",
	Callback = function()
		Rayfield:Destroy()
	end,
})

local Button = Tab:CreateButton({
	Name = "Make By Minh Lts",
	Callback = function()
		setclipboard("facebook : minh lts")
	end,
})

local fix = Window:CreateTab("Fix Lag")

local Button = fix:CreateButton({
	Name = "Fix Lag",
	Callback = function()
		loadstring(game:HttpGet("https://pastebin.com/raw/eHEfAR8z", true))()
	end,
})

